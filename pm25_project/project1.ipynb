  Returns:\n","    - tuple: (predicted_pm25_value, category_string)\n","    \"\"\"\n","\n","    # Load CSV from package\n","    data = pd.read_csv(\"pm25_predictor/PRSA_data_2010.1.1-2014.12.31.csv\")\n","    data[\"pm2.5\"] = data[\"pm2.5\"].fillna(data[\"pm2.5\"].mean())\n","\n","    # Encode wind direction\n","    le = LabelEncoder()\n","    data['cbwd'] = le.fit_transform(data['cbwd'])\n","\n","    # Features and target\n","    x = data.drop(columns=[\"pm2.5\",\"datetime\",'Ir','Is','No','year'], errors='ignore')\n","    y = data[\"pm2.5\"]\n","\n","    # Train-test split\n","    x_train, x_test, y_train, y_test = train_test_split(x, y, test_size=0.2, random_state=42)\n","\n","    # Train RandomForest model\n","    model = RandomForestRegressor(random_state=42, n_estimators=500,\n","                                  min_samples_split=2,\n","                                  min_samples_leaf=1,\n","                                  max_features=None,\n","                                  max_depth=20)\n","    model.fit(x_train, y_train)\n","\n","    # Encode cbwd in new data\n","    weather_data['cbwd'] = le.transform(weather_data['cbwd'])\n","\n","    # Align columns\n","    weather_data = weather_data.reindex(columns=x_train.columns, fill_value=0)\n","\n","    # Predict and scale\n","    predicted_pm25 = model.predict(weather_data) * scale_factor\n","\n","    # Convert to category\n","    def pm25_to_category(value):\n","        if value <= 12:\n","            return \"Good\"\n","        elif value <= 35:\n","            return \"Moderate\"\n","        elif value <= 55:\n","            return \"Unhealthy for Sensitive Groups\"\n","        elif value <= 150:\n","            return \"Unhealthy\"\n","        elif value <= 250:\n","            return \"Very Unhealthy\"\n","        else:\n","            return \"Hazardous\"\n","\n","    category = pm25_to_category(predicted_pm25[0])\n","\n","    return predicted_pm25[0], category\n"]},{"cell_type":"code","source":["